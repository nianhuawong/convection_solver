    YAxisVar = 2
    }
  Lines
    {
    Color = Purple
    LineThickness = 0.3
    }
  Symbols
    {
    SymbolShape
      {
      GeomShape = Diamond
      }
    Color = Purple
    FillMode = UseLineColor
    FillColor = Red
    Size = 1.5
    }
  BarCharts
    {
    Color = Red
    FillColor = Red
    }
  ErrorBars
    {
    Color = Red
    }
$!LineMap  [5]
  Name = 'CTCS'
  Assign
    {
    Zone = 5
    XAxisVar = 1
    YAxisVar = 2
    }
  Lines
    {
    Color = Cyan
    LineThickness = 0.3
    }
  Symbols
    {
    SymbolShape
      {
      GeomShape = Circle
      }
    Color = Cyan
    FillMode = UseLineColor
    FillColor = Red
    Size = 1.5
    }
  BarCharts
    {
    Color = Red
    FillColor = Red
    }
  ErrorBars
    {
    Color = Red
    }
$!LineMap  [6]
  Name = '2nd_upwind'
  Assign
    {
    Zone = 6
    XAxisVar = 1
    YAxisVar = 2
    }
  Lines
    {
    Color = Black
    LineThickness = 0.3
    }
  Symbols
    {
    Color = Green
    FillMode = UseLineColor
    FillColor = Red
    Size = 1.5
    }
  BarCharts
    {
    Color = Red
    FillColor = Red
    }
  ErrorBars
    {
    Color = Red
    }
$!XYLineAxis 
  DepXToYRatio = 1
$!XYLineAxis 
  XDetail 1
    {
    RangeMin = 0
    RangeMax = 6.2831999999999999
    AutoGrid = No
    GRSpacing = 1.5708
    Gridlines
      {
      Show = Yes
      }
    MinorGridlines
      {
      Show = Yes
      }
    }
$!XYLineAxis 
  YDetail 1
    {
    RangeMin = -1.2
    RangeMax = 1.5
    GRSpacing = 0.5
    Gridlines
      {
      Show = Yes
      }
    MinorGridlines
      {
      Show = Yes
      }
    }
$!FrameControl ActivateByNumber
  Frame = 1
$!SetStyleBase Config
