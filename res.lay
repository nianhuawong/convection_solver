    {
    Zone = 3
    XAxisVar = 1
    YAxisVar = 2
    }
  Lines
    {
    Color = Blue
    LineThickness = 0.3
    }
  Symbols
    {
    SymbolShape
      {
      GeomShape = Grad
      }
    Color = Blue
    FillMode = UseLineColor
    FillColor = Red
    Size = 1.5
    }
  BarCharts
    {
    Color = Red
    FillColor = Red
    }
  ErrorBars
    {
    Color = Red
    }
